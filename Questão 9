#include <iostream> 
#include <cmath> 
using namespace std;

//----QUESTÃO 9----//

void questao9(int a){
  int i=1, soma=0;
 
 while (i<=a){
    if(a%i==0){
      cout << "divisores: " << i <<"\n";
    }
    i=i+1;
  }
}

int main(){
  int a;

  cout << "digite um número inteiro:\n";
  cin >> a;

  questao9(a);
}
