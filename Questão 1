#include <iostream> 
#include <cmath> 
using namespace std;
//----QUESTÃO 1----//

void questao1(){
  int N, soma; 
  
  cout << "Digite um número inteiro\n"; 
  
  cin >> N; 

  for (int i=1;i<=N;i++){
 
    soma = i + N;
  }
 
  cout << "Soma dos números:" << soma;
}

int main() {

  questao1();
  
}
